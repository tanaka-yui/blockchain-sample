{
  "name": "front",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build": "tsc && vite build",
    "dev:node1": "export VITE_PORT=3000 && export VITE_TARGET=0.0.0.0:8080 && vite",
    "dev:node2": "export VITE_PORT=3001 && export VITE_TARGET=0.0.0.0:8081 && vite",
    "format": "prettier --write './**/*.{js,jsx,ts,tsx,css,md,json}' ",
    "lint": "eslint --fix './**/*.{js,ts,tsx}' --ignore-path ../.gitignore",
    "preview": "vite preview",
    "sort": "sort-package-json 'package.json'",
    "type-check": "tsc",
    "update-deps": "ncu -u"
  },
  "dependencies": {
    "@emotion/react": "11.9.0",
    "@emotion/styled": "11.8.1",
    "@mui/icons-material": "5.6.2",
    "@mui/material": "5.6.4",
    "react": "18.1.0",
    "react-dom": "18.1.0",
    "react-router-dom": "6.3.0"
  },
  "devDependencies": {
    "@babel/core": "7.17.10",
    "@types/react": "18.0.9",
    "@types/react-dom": "18.0.3",
    "@typescript-eslint/eslint-plugin": "5.22.0",
    "@typescript-eslint/parser": "5.22.0",
    "@vitejs/plugin-react": "1.3.2",
    "eslint": "8.15.0",
    "eslint-config-prettier": "8.5.0",
    "eslint-plugin-import": "2.26.0",
    "eslint-plugin-jsx-a11y": "6.5.1",
    "eslint-plugin-prettier": "4.0.0",
    "eslint-plugin-react": "7.29.4",
    "eslint-plugin-simple-import-sort": "7.0.0",
    "npm-check-updates": "12.5.11",
    "prettier": "2.6.2",
    "sort-package-json": "1.57.0",
    "typescript": "4.6.4",
    "vite": "2.9.8"
  },
  "engines": {
    "node": "18.3.0",
    "pnpm": "7.1.9"
  }
}
